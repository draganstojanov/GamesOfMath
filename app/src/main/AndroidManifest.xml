<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.andraganoid.gameofmath">

    <application
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Main"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Practice.PracticeSettings"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Practice.PracticeBoard"
            android:screenOrientation="portrait" />
        <activity
            android:name=".GameBoard"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Arcade.ArcadeBoard"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Arcade.ArcadeSettings"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Heavy.HeavySettings"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Heavy.HeavyBoard"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Easy.EasySettings"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Easy.EasyBoard"
            android:screenOrientation="portrait" />

        <activity android:name=".Game"
        android:screenOrientation="portrait" />

        <meta-data android:name="com.google.android.gms.games.APP_ID"
            android:value="@string/app_id" />
        <meta-data android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version"/>
    </application>

</manifest>